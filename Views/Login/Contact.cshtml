@model StudentCollab.Models.User
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Contact</h2>

Details: <br />

User Name: @Model.UserName <br />
PassWord: @Model.Password <br />
@if(Model.rank == 1)
{
    <i>Rank: Basic User</i>
}
else if(Model.rank == 0)
{
    <i>Rank: Admin</i>
}
else if(Model.rank == 2)
{
    <i>Rank: Manager</i>
}
